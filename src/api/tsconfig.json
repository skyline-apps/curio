{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "lib": [
      "ESNext"
    ],
    "paths": {
      "@api/*": [
        "./*"
      ]
    },
    "rootDir": ".",
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.mts"
  ]
}